{
	"name": "portfolio",
	"version": "1.0.0",
	"private": true,
	"dependencies": {
		"@types/node": "^12.20.20",
		"@types/react": "^17.0.19",
		"@types/react-dom": "^17.0.9",
		"@typescript-eslint/eslint-plugin": "^4.29.2",
		"@typescript-eslint/parser": "^4.29.2",
		"eslint": "^7.32.0",
		"eslint-config-marine": "^9.0.6",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.1",
		"prettier": "^2.3.2",
		"prettier-eslint-cli": "^5.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-parallax": "^3.3.0",
		"react-scripts": "4.0.3",
		"react-scroll-parallax": "^2.4.0",
		"sass": "^1.38.0",
		"typescript": "^4.3.5"
	},
	"scripts": {
		"start": "sass --no-source-map --watch src/ & react-scripts start",
		"build": "sass --no-source-map src/ && react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "prettier-eslint --write \"src/**/*.ts\" \"src/**/*.scss\" \"src/**/*.tsx\" \"./**/*.json\""
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
